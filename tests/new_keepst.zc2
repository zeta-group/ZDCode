class JumpyTorch extends RedTorch {
    macro Jump {
        #### A 0 ThrustThingZ(0, 50, 0, 1);
        #### BAB 3 [Bright];
        #### A 5 [Bright];
        #### A 20;
        TRED A 2;
    }

    label Spawn {
        x3 TRED ABCD 4 [Bright];
        sometimes 5 inject Jump;
        loop;
    }
}

